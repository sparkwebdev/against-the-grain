<!doctype html>
<html {% if section %} data-current="{{ section }}"{% endif %} lang="en">

	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>{{ title or renderData.title or metadata.title }}</title>
		{% set css %}
		{% include "assets/css/inline.css" %}
		{% endset %}
		<style>{{ css | cssmin | safe }}</style>
		{% set js %}
		{% include "assets/js/inline.js" %}
		{% endset %}
		<script>{{ js | jsmin | safe }}</script>
	</head>

	<link rel="stylesheet" href="https://use.typekit.net/cne1szk.css">

	<body>

		<main>
			{{ layoutContent | safe }}
		</main>

	</body>

</html>
